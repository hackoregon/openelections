{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "dependencies": {
    "@hackoregon/component-library": "^3.0.0-ci",
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "^4.2.0",
    "clsx": "^1.0.4",
    "connected-react-router": "^6.5.2",
    "formik": "^1.5.4",
    "history": "^4.9.0",
    "jwt-decode": "^2.2.0",
    "material-table": "^1.40.0",
    "normalizr": "^3.3.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^3.0.0",
    "react-transition-group": "^4.0.0",
    "redux": "^4.0.1",
    "redux-flash": "^2.0.2",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "yup": "^0.27.0"
  },
  "scripts": {
    "start": "PORT=4000 react-scripts start",
    "start-windows": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "flow": "flow"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/node_modules/regenerator-runtime/runtime",
      "./test/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "snapshotResolver": "<rootDir>/snapshotResolver.js"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@emotion/core": "^10.0.10",
    "@storybook/addon-a11y": "^5.0.10",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-knobs": "^5.0.10",
    "@storybook/addon-links": "^5.0.6",
    "@storybook/addon-notes": "^5.0.10",
    "@storybook/addon-storyshots": "^5.1.9",
    "@storybook/addon-storysource": "^5.0.10",
    "@storybook/addons": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "babel-jest": "^24.8.0",
    "babel-loader": "8.0.5",
    "babel-preset-env": "^1.7.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.0",
    "enzyme-to-json": "^3.3.5",
    "fetch-vcr": "^3.0.0",
    "identity-obj-proxy": "^3.0.0",
    "react-test-renderer": "^16.8.6",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
